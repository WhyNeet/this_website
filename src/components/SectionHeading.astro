---
interface Props {
  title: string;
  description: string;
}

const { description, title } = Astro.props;
---

<p class="text-secondary font-bold text-xl mb-3">{description}</p>
<h1
  class="text-5xl bg-gradient-to-b from-text to-primary bg-clip-text text-transparent font-bold mb-16"
>
  {title}
</h1>
